            <string>///buying things

if(type == 1){     //SHOTGUN
    if(global.weapons &gt; 0){
        if(global.points &gt;= global.weaponArray[type,16]){
            global.shotgunAmmo +=2;
            global.points -= global.weaponArray[type,16];
            scr_saveGame();
        }
    }else if(global.weapons == 0){
        if(global.points &gt;= global.weaponArray[type,15]){
            //add weapon from weapon Array
            global.weapons = 1;
            
            if(!global.mission3){
                box = instance_create(x,y,obj_missions);
                box.type = 3;
                global.mission3 = true;
                global.points += 200;
            }
            
            //take cash
            global.points -= global.weaponArray[type,15];
            global.shotgunAmmo = 10;
         scr_saveGame();   
        }
    }
}else if(type == 2){   //MACHINEGUN
    if(global.weapons == 1) {
        if(global.points &gt;= global.weaponArray[type,15]){
                //add weapon from weapon Array
                global.weapons = 2;
                
                if(!global.mission4){
                    box = instance_create(x,y,obj_missions);
                    box.type = 4;
                    global.mission4 = true;
                    global.points += 500;
                }
               
                //take cash
                global.points -= global.weaponArray[type,15];
                global.machineAmmo = 30;
             scr_saveGame();   
            }
    }else if(global.weapons &gt; 1){
        if(global.points &gt;= global.weaponArray[type,16]){
            global.machineAmmo +=30;
            global.points -= global.weaponArray[type,16];
            scr_saveGame();
        }
    }
}else if(type == 3){     //HEALTH
    if(global.points &gt;= global.healthPrice){
           global.playerHp += 10;
           global.points -= global.healthPrice;
           global.healthPrice*=2;
           scr_saveGame();
    }
}else if(type == 4){     //JUMP
    if(global.playerJump &lt;= 30){
        if(global.points &gt;= global.jumpPrice){
               global.playerJump += 5;
               with(obj_player){
                    grav +=.5;
               }
               global.points -= global.jumpPrice;
               global.jumpPrice*=8;
               scr_saveGame();
        }
    }
}else if(type == 5){     //SPEED
    if(global.playerSpeed &lt;= 11){
        if(global.points &gt;= global.speedPrice){
               global.playerSpeed ++;
               global.points -= global.speedPrice;
               global.speedPrice*=4;
               scr_saveGame();
        }
    }
}else if(type == 6){     //SHIELD
    if(global.points &gt;= global.shieldPrice){
           global.time += 2;
           global.points -= global.shieldPrice;
           global.shieldPrice*=2;
           scr_saveGame();
    }
}


