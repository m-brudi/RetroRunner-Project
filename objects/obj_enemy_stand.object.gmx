if(global.death) exit;
// Horizontal collisions
if (place_meeting(x+hspd, y, obj_ground)) {
    while (!place_meeting(x+sign(hspd), y, obj_ground)) {
        x += sign(hspd);
    }
    hspd = 0; 
}

// Move horizontally 
x += hspd;

vspd += grav;

// Vertical collisions
if (place_meeting(x, y+vspd, obj_ground)) {
    while (!place_meeting(x, y+sign(vspd), obj_ground)) {
        y += sign(vspd);
    }
    vspd = 0; 
}
y += vspd;



if(hp &lt;1){
    scr_enemyDeath(x,y);
}


if(x &lt; obj_player.x-300) then instance_destroy();

if(obj_player.x &lt; x){
image_xscale = -1;
}else{
image_xscale = 1;
}
